        cov_1ibl20y9gd().s[28]++;
        // Handle email/password flow with real Firebase
        console.log(`üîê Attempting email/password sign-in for: ${email}`);
        /* istanbul ignore next */
        cov_1ibl20y9gd().s[29]++;
        try {
          const signInResult =
          /* istanbul ignore next */
          (cov_1ibl20y9gd().s[30]++, await firebase_user_service_1.firebaseUserService.signInWithEmailAndPassword(email, password));
          /* istanbul ignore next */
          cov_1ibl20y9gd().s[31]++;
          authResult = {
            success: true,
            user: {
              uid: signInResult.user.uid,
              email: signInResult.user.email,
              name: signInResult.user.displayName,
              email_verified: signInResult.user.emailVerified
            }
          };
          // Use the custom token from Firebase
          /* istanbul ignore next */
          cov_1ibl20y9gd().s[32]++;
          sessionToken = signInResult.token;
          /* istanbul ignore next */
          cov_1ibl20y9gd().s[33]++;
          console.log(`‚úÖ Email/password sign-in successful for uid: ${authResult.user.uid}`);
        } catch (error) {
          /* istanbul ignore next */
          cov_1ibl20y9gd().s[34]++;
          console.error('‚ùå Firebase email/password sign-in failed:', error);
          /* istanbul ignore next */
          cov_1ibl20y9gd().s[35]++;
          return server_1.NextResponse.json({
            error: 'Invalid email or password'
          }, {
            status: 401
          });
        }
      } else {
        /* istanbul ignore next */
        cov_1ibl20y9gd().b[9][1]++;
        cov_1ibl20y9gd().s[36]++;
        return server_1.NextResponse.json({
          error: 'Email and password or ID token is required'
        }, {
          status: 400
        });
      }
    }
    // Ensure user is properly authenticated
    /* istanbul ignore next */
    cov_1ibl20y9gd().s[37]++;
    if (
    /* istanbul ignore next */
    (cov_1ibl20y9gd().b[12][0]++, !authResult.user) ||
    /* istanbul ignore next */
    (cov_1ibl20y9gd().b[12][1]++, !authResult.user.uid)) {
      /* istanbul ignore next */
      cov_1ibl20y9gd().b[11][0]++;
      cov_1ibl20y9gd().s[38]++;
      console.error('‚ùå Authentication failed - no user data');
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[39]++;
      return server_1.NextResponse.json({
        error: 'Authentication failed'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1ibl20y9gd().b[11][1]++;
    }
    cov_1ibl20y9gd().s[40]++;
    try {
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[41]++;
      // Ensure user profile exists in Firestore
      console.log(`üîç Ensuring user profile exists for uid: ${authResult.user.uid}`);
      const userProfile =
      /* istanbul ignore next */
      (cov_1ibl20y9gd().s[42]++, await firebase_user_service_1.firebaseUserService.ensureUserProfile(authResult.user.uid, {
        email: authResult.user.email,
        displayName: authResult.user.name,
        emailVerified: authResult.user.email_verified
      }));
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[43]++;
      console.log(`‚úÖ User profile confirmed for uid: ${authResult.user.uid}`);
      // Set session cookie with the actual token
      const cookieStore =
      /* istanbul ignore next */
      (cov_1ibl20y9gd().s[44]++, await (0, headers_1.cookies)());
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[45]++;
      cookieStore.set(SESSION_COOKIE_NAME, sessionToken, {
        httpOnly: true,
        secure: process.env.NODE_ENV === 'production',
        maxAge: SESSION_DURATION_S,
        path: '/',
        sameSite: 'lax'
      });
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[46]++;
      console.log(`üç™ Session cookie set for uid: ${authResult.user.uid}`);
      // Return user data with auth token for localStorage
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[47]++;
      return server_1.NextResponse.json({
        success: true,
        token: sessionToken,
        // Include token for frontend storage
        user: {
          uid: userProfile.uid,
          email: userProfile.email,
          name: userProfile.displayName,
          email_verified: userProfile.emailVerified,
          plan: userProfile.plan,
          profilePictureUrl: userProfile.profilePictureUrl,
          createdAt: userProfile.createdAt,
          updatedAt: userProfile.updatedAt
        }
      });
    } catch (profileError) {
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[48]++;
      console.error('‚ùå Failed to handle user profile:', profileError);
      /* istanbul ignore next */
      cov_1ibl20y9gd().s[49]++;
      return server_1.NextResponse.json({
        error: 'Failed to create or retrieve user profile'
      }, {
        status: 500
      });
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_1ibl20y9gd().s[50]++;
    console.error('‚ùå Signin error:', error);
    /* istanbul ignore next */
    cov_1ibl20y9gd().s[51]++;
    return server_1.NextResponse.json({
      error: 'Internal server error'
    }, {
      status: 500
    });
  }
}

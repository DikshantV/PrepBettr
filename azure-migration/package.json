{
  "name": "azure-migration",
  "version": "1.0.0",
  "description": "Provider-agnostic Firebase to Azure migration library",
  "main": "lib/index.ts",
  "types": "lib/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'lib/**/*.ts'",
    "lint:fix": "eslint 'lib/**/*.ts' --fix",
    "health-check": "ts-node scripts/health-check.ts",
    "migrate": "ts-node scripts/migrate.ts"
  },
  "keywords": [
    "azure",
    "firebase",
    "migration",
    "cloud",
    "typescript"
  ],
  "author": "PrepBettr Migration Team",
  "license": "MIT",
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/app-configuration": "^1.5.0",
    "@azure/msal-node": "^2.6.6",
    "firebase-admin": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "firebase-admin": ">=11.0.0"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "MIGRATION_GUIDE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/azure-migration.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

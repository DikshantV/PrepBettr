{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetrics": true
    },
    "logLevel": {
      "default": "Information",
      "Host.Aggregator": "Trace",
      "Microsoft.Azure.WebJobs.Script.WebHost.Diagnostics.SystemLoggerProvider": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "functionTimeout": "00:10:00",
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "maximumFunctionConcurrency": 5
  },
  "http": {
    "routePrefix": "api",
    "maxOutstandingRequests": 500,
    "maxConcurrentRequests": 200,
    "dynamicThrottlesEnabled": true
  },
  "queues": {
    "maxPollingInterval": "00:00:02",
    "visibilityTimeout": "00:02:00",
    "batchSize": 8,
    "maxDequeueCount": 3,
    "newBatchThreshold": 4
  },
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 2,
    "minimumInterval": "00:00:05",
    "maximumInterval": "00:02:00"
  },
  "managedDependency": {
    "enabled": true
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "node",
      "workingDirectory": "",
      "arguments": []
    },
    "enableForwardingHttpRequest": false
  },
  "healthMonitor": {
    "enabled": true,
    "healthCheckInterval": "00:00:10",
    "healthCheckWindow": "00:02:00",
    "healthCheckThreshold": 6,
    "counterThreshold": 0.80
  }
}
